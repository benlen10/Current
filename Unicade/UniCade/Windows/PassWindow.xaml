<Window x:Class="UniCade.Windows.PassWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Enter Password" Height="130.046" Width="300" ResizeMode="NoResize">
    <Grid x:Name="ButtonClose">
        <Label x:Name="LabelPassword" Content="Password" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="75" Click="PassWindow_CloseButton_Click"/>
        <Button Content="Enter" HorizontalAlignment="Left" Margin="195,67,0,0" VerticalAlignment="Top" Width="75" Click="PassWindow_EnterButton_Click"/>
        <PasswordBox x:Name="TextboxPassword" HorizontalAlignment="Left" Margin="74,28,0,0" VerticalAlignment="Top" Height="22" Width="196"/>

    </Grid>
</Window>
