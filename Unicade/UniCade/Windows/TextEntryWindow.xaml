<Window x:Class="UniCade.Windows.TextEntryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Text Entry Window" Height="110.843" Width="300" ResizeMode="NoResize">
    <Grid Margin="0,0,0,-2.5">
        <TextBox x:Name="TextEntryWindowTextbox" HorizontalAlignment="Left" Height="23" Margin="10,21,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="TextEntryWindowButtonClose" Content="Close" HorizontalAlignment="Left" Height="24" Margin="10,49,0,-23.5" VerticalAlignment="Top" Width="96" Click="TextEntryWindowButtonClose_Click"/>
        <Button x:Name="TextEntryWindowButtonConfirm" Content="Confirm" HorizontalAlignment="Left" Height="24" Margin="188,49,0,-23.5" VerticalAlignment="Top" Width="96" Click="TextEntryWindowButtonConfirm_Click"/>

    </Grid>
</Window>
