<Window x:Class="UniCade.Windows.PassWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UniCade.Windows"
        mc:Ignorable="d"
        Title="Enter Password" Height="130.046" Width="300">
    <Grid x:Name="Button_Close">
        <TextBox x:Name="Textbox_Password" HorizontalAlignment="Left" Height="23" Margin="74,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="196"/>
        <Label x:Name="Label_Password" Content="Password" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top"/>
        <Button Content="Close" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" Width="75" Click="PassWindow_CloseButton_Click"/>
        <Button Content="Enter" HorizontalAlignment="Left" Margin="195,67,0,0" VerticalAlignment="Top" Width="75" Click="PassWindow_EnterButton_Click"/>

    </Grid>
</Window>
