            </TabItem>
            <TabItem Header="Emulators">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="Users">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="Global">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="Web">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="UniCade Cloud">
                <Grid Background="White"/>
            </TabItem>
            <TabItem Header="About">
                <Grid Background="White"/>
            </TabItem>
        </TabControl>


    </Grid>
</Window>
