                    <Label Content="Title" HorizontalAlignment="Left" Margin="374,55,0,0" VerticalAlignment="Top"/>
                    <Label Content="Console" HorizontalAlignment="Left" Margin="374,92,0,0" VerticalAlignment="Top"/>
                    <Label Content="Release Date" HorizontalAlignment="Left" Margin="374,118,0,0" VerticalAlignment="Top"/>
                    <Label Content="Publisher" HorizontalAlignment="Left" Margin="374,144,0,0" VerticalAlignment="Top"/>
                    <Label Content="Developer" HorizontalAlignment="Left" Margin="374,170,0,0" VerticalAlignment="Top"/>
                    <Label Content="ESRB" HorizontalAlignment="Left" Margin="374,196,0,0" VerticalAlignment="Top"/>
                    <Label Content="ESRB Descriptor" HorizontalAlignment="Left" Margin="374,222,0,0" VerticalAlignment="Top"/>
                    <Label Content="Game Description" HorizontalAlignment="Left" Margin="374,248,0,0" VerticalAlignment="Top"/>
